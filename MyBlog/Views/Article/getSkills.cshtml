    @model IEnumerable<MyBlog.DataEntitiess.SKILLS>
<div class="skils-content" style="margin: 30px; padding: 3px;">

    @foreach (var item in Model)
    {
        <div class="skillbar clearfix " data-percent="@item.SKILLYUZDE%">
            <div class="skillbar-title" style="background: @item.SKILL_TITLE_BG;"><span>@item.SKILLTITLE</span></div>
            <div class="skillbar-bar" style="background: @item.SKILL_BAR_BG;"></div>
            <div class="skill-bar-percent">@item.SKILLYUZDE%</div>
        </div> <!-- End Skill Bar -->
    }
</div>









