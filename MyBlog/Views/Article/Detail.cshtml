    <script type="text/javascript">

        $('#submit').click(function () {
            var Comment = $('#Comment').val();
            var CommentByName = $('#CommentByName').val();
            var EPOSTA = $('#EPOSTA').val();
            var WEBSITE = $('#WEBSITE').val();
            var MAKALEID = $('#MAKALEID').val();
            var USTID = $('#USTID').val();

            if (CommentByName == "") {
                ShowMessage('Lütfen isim alanını doldurunuz !', 'e');
            }
            else if (EPOSTA == "") {
                ShowMessage('Lütfen E-Posta alanını doldurunuz !', 'e');
            }
            else {
                $.ajax({
                    url: '/Article/postComment',
                    type: 'POST',
                    dataType: 'json',
                    data: {
                        Comment: Comment,
                        CommentByName: CommentByName,
                        EPOSTA: EPOSTA,
                        WEBSITE: WEBSITE,
                        MAKALEID: MAKALEID,
                        USTID: USTID
                    },
                    success: function (response) {
                        ShowMessage(response.CommentByName + ' Yorumun gönderildi. Onaylandıktan sonra paylaşılacaktır.', 's');
                    },
                    error: function (hata) {
                        ShowMessage('Yourumun gönderilirken bir hata oluştu lütfen tekrar dene.', 'e');
                    }
                })
            }


        });



        function ShowMessage(message, type) {
            if (type == "s") {
                swal({
                    title: 'İşlem Başarılı',
                    text: message,
                    type: 'success',
                    showCancelButtonClass: 'btn-success',
                    confirmButtonText: 'Tamam'
                });
            }
            else {
                swal({
                    title: 'İşlem Başarısız',
                    text: message,
                    type: 'error',
                    showCancelButtonClass: 'btn-danger',
                    confirmButtonText: 'Tamam'
                });
            }
        }
    </script>
